{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "consistent-return": "off",
    "comma-dangle": "off",
    "import/prefer-default-export": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always"
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_d", "_dh", "_h", "_id", "_m", "_n", "_t", "_text", "_doc"]
      }
    ],
    "object-curly-newline": "off",
    "operator-linebreak": "off"
  }
}
